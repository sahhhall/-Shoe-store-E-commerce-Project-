<script>
  function deleteCartItem(proId){
     
              $.ajax({
                  url:"/removeCartitem",
                  data:{
                      product:proId
                  },
                  method:"post",
                  success:(response)=>{
                      if((response.success = true )){
                        // $('#reloadArea').load(' #reloadArea'); 
                        location.reload();
                         
                      }
                  }
              })
       
  }
</script>
